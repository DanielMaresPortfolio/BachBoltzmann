<Window x:Class="BachBoltyman.Results"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BachBoltyman"
        mc:Ignorable="d"
        Title="Results" Height="500" Width="500">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="9*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
        </Grid.RowDefinitions>

        <ListBox x:Name="TimeSelect" SelectionChanged="ListBox_SelectionChanged" ItemsSource="{Binding AllTime}" />
        <Image x:Name="Heatmap" Grid.Column="1"/>
    </Grid>
</Window>
